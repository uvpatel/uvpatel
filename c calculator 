#include <stdio.h>

int main() {
  
  float a;
  printf("Enter a number: ");
  scanf("%f",&a);
  
  float b;
  printf("Enter b number: ");
  scanf("%f",&b);

  
  
  float sum;
  float sub;
  float mul;
  float div;
  sum=a+b;
  sub=a-b;
  mul=a*b;
  div=a/b;
  char op;

  printf("Enter an operator: ");
  scanf("%c",&op);
  
    if(op=='+'){
     printf("the sum            is:%f\n",sum);}
    else if(op=='-'){
     printf("the sub is:%f\n",sub);}
     else if( op=='*'){
      printf("the mul is:%f\n",mul);}
     else if(op=='/'){
     printf("the div   is:%f\n",div);}
  return 0;
}